{
  "name": "@axway/amplify-central-layer7-extension",
  "version": "0.0.1-dev.1",
  "description": "Create AMPLIFY Central resources from layer7 resources",
  "license": "Apache-2.0",
  "author": {
    "email": "support@axway.com",
    "name": "Axway",
    "url": "https://axway.com"
  },
  "scripts": {
    "build": "npm run tsc && npm run copy:assets",
    "copy:assets": "npx cpx 'src/assets/**' 'dist/src/assets'",
    "test": "ts-mocha --timeout 10000",
    "test:coverage": "nyc yarn test",
    "postinstall": "npm i --only=dev --ignore-scripts && npm run build && node ./postinstall.js",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">= 8.9"
  },
  "main": "./dist/main.js",
  "amplify": {
    "type": "central-cli-plugin"
  },
  "cli-kit": {
    "main": "./dist/cli.js",
    "name": "layer7-extension"
  },
  "keywords": [
    "axway",
    "amplify",
    "apicentral",
    "central",
    "amplify-package",
    "cli-kit"
  ],
  "aliases": [
    "layer7"
  ],
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.2",
    "@axway/amplify-cli-utils": "^4.2.5",
    "cli-kit": "^1.8.2",
    "fs-extra": "^9.0.1",
    "https": "^1.0.0",
    "https-proxy-agent": "^2.2.4",
    "js-yaml": "^3.14.0",
    "mime-types": "^2.1.27",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/fs-extra": "^9.0.2",
    "@types/js-yaml": "^3.12.5",
    "@types/mime-types": "^2.1.0",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@types/sinon": "^9.0.8",
    "chai": "^4.2.0",
    "mocha": "^10.1.0",
    "nock": "^13.0.4",
    "nyc": "^15.1.0",
    "openapi-types": "^7.0.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.0",
    "ts-mocha": "^7.0.0",
    "typescript": "^4.0.3"
  }
}
